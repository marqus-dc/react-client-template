{
  "name": "react-client-template",
  "version": "0.1.0",
  "description": "React web client template based on facebook's react-script",
  "repository": {
    "type": "git",
    "url": "https://github.com/marqusm/react-client-template.git"
  },
  "keywords": [
    "react",
    "client",
    "template",
    "web"
  ],
  "author": "Marko Mišković <misko23@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marqusm/react-client-template/issues"
  },
  "homepage": "https://github.com/marqusm",
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "font-awesome": "^4.7.0",
    "history": "^4.6.3",
    "material-ui": "^0.19.1",
    "moment": "^2.18.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "react-scripts": "1.0.13",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-form-material-ui": "^4.2.0",
    "redux-thunk": "^2.2.0",
    "sha1": "^1.1.1"
  }
}
